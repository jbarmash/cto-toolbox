<h1>Listing Users</h1>

<table class="table">
  <thead>
    <th>Name</th>
      <!--
    <th>Provider</th>
    <th>UID</th>
    -->
  </thead>

<% @users.each do |user| %>
  <tr>
      <td><%= link_to user.name, user_path(user) 
          %></td>
      <!--
    <td><%= user.provider %></td>
    <td><%= user.uid %></td>
    -->
    
    <td>(<%= link_to "Set as current", user_set_current_user_path(user)%>)</td>
  </tr>
<% end %>
  <tfoot>
  </tfoot>
</table>

